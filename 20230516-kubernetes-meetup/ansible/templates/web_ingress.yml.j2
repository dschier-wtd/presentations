---
apiVersion: "networking.k8s.io/v1"
kind: "Ingress"
metadata:
  name: "web-ingress"
  namespace: "web"
spec:
  rules:
    - http:
        paths:
          - pathType: "Prefix"
            path: "/"
            backend:
              service:
                name: "web-service"
                port:
                  number: 80
...
